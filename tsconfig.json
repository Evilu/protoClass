{
  "compilerOptions": {
    "incremental": true,
    "target": "es2021",
    "module": "commonjs",
    "lib": [
      "es2021",
      "dom"
    ],
    "strict": true,
    "sourceMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build"
  },
  "exclude": [
    "protos/**"
  ]
}
